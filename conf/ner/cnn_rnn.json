{
  "model":{
    "arch":"cnn_rnn",
    "weight_init":"xavier",
    "use_chars":true,
    "c_dim":100,
    "c_in_dropout":0.0,
    "c_cnn_layers":[
      {"height":3,"filters":128,"dilation":1}
    ],
    "pretrained":null,
    "w_dim":200,
    "w_in_dropout":0.0,
    "w_rnn_out":"allsteps",
    "w_rnn_layers":[
      {"cell":"lstm","dim":300,"n_layers":1,"bidirectional":true}
    ],
    "mid_dropout":0.0,
    "use_crf":true
  },
  "optim":{
    "method":"adam",
    "lr":0.001,
    "momentum":0.0,
    "l2_loss":0.0,
    "lr_scheduler":null,
    "step":25,
    "decay":1.0
  },
  "train":{
    "batch_size":256,
    "max_epochs":500,
    "val_steps":5,
    "patience":5
  }
}
